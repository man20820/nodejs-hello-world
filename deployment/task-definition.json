{
    "family": "nodejs-hello-world",
    "containerDefinitions": [
        {
            "name": "nodejs-hello-world",
            "image": "763572795622.dkr.ecr.ap-southeast-3.amazonaws.com/nodejs-hello-world:test",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "nodejs-hello-world-3000-tcp",
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/nodejs-hello-world",
                    "awslogs-create-group": "true",
                    "awslogs-region": "ap-southeast-3",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "taskRoleArn": "arn:aws:iam::763572795622:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::763572795622:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}